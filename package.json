{
  "name": "@kaviar/x-bundle",
  "version": "1.3.0",
  "description": "X-way is a feature-rich solution with fast prototyping in mind for KaviarJS",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kaviarjs/x-bundle"
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run compile",
    "test": "jest --verbose dist/__tests__/index.js --passWithNoTests",
    "test:clean": "jest --clearCache",
    "test:watch": "jest --verbose --watch dist/__tests__/index.js",
    "testonly": "npm test",
    "test:ci": "npm run coverage -- --ci --maxWorkers=2  --reporters=default --reporters=jest-junit",
    "coverage": "npm test -- --coverage",
    "coverage:upload": "codecov",
    "prepublishOnly": "npm run compile",
    "gen-doc": "typedoc ./src/index.ts --exclude ./src/__tests__ --out typeDocs --tsconfig tsconfig.json"
  },
  "dependencies": {
    "@kaviar/ejson": "^1.2.0",
    "chalk": "^4.1.0",
    "date-fns": "^2.14.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-type-json": "^0.3.2",
    "queue": "^6.0.2",
    "redis": "^3.0.2"
  },
  "peerDependencies": {
    "@kaviar/core": "^1.1.1",
    "@kaviar/nova": "^1.3.2",
    "@kaviar/mongo-bundle": "^1.0.0",
    "@kaviar/validator-bundle": "^1.0.0",
    "@kaviar/security-bundle": "^1.0.0",
    "@kaviar/logger-bundle": "^1.0.0",
    "@kaviar/graphql-bundle": "^1.0.0",
    "graphql": "^15.3.0 || ^14.0.0"
  },
  "devDependencies": {
    "@kaviar/apollo-bundle": "^1.1.0",
    "@kaviar/core": "^1.1.1",
    "@kaviar/email-bundle": "^1.0.0",
    "@kaviar/graphql-bundle": "^1.0.0",
    "@kaviar/logger-bundle": "^1.0.0",
    "@kaviar/mongo-bundle": "^1.0.0",
    "@kaviar/nova": "^1.3.2",
    "@kaviar/security-bundle": "^1.0.0",
    "@kaviar/validator-bundle": "^1.0.0",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/graphql": "^0.11.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^8.0.28",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "class-transformer": "^0.3.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "graphql": "^15.4.0",
    "jest": "^26.0.0",
    "jest-junit": "^10.0.0",
    "mongodb": "^3.6.0",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.13",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2"
  },
  "types": "dist/index.d.ts",
  "license": "MIT"
}
